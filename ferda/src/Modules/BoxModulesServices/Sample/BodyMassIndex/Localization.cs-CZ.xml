    <Phrase>
      <PhraseIdentifier>Underweight</PhraseIdentifier>
      <PhraseText>Hubená postava [15 - 20]</PhraseText>
    </Phrase>
    <Phrase>
      <PhraseIdentifier>Normal</PhraseIdentifier>
      <PhraseText>Normalní postava [20 - 25]</PhraseText>
    </Phrase>
    <Phrase>
      <PhraseIdentifier>Overweight</PhraseIdentifier>
      <PhraseText>Postava s nadváhou [25 - 30]</PhraseText>
    </Phrase>
    <Phrase>
      <PhraseIdentifier>Obese</PhraseIdentifier>
      <PhraseText>Obézní postava [&gt; 30]</PhraseText>
    </Phrase>
    <Phrase>
      <PhraseIdentifier>BodyMassIndex</PhraseIdentifier>
      <PhraseText>BMI - index tělexné hmotnosti</PhraseText>
    </Phrase>
  </Phrases>
</BoxLocalization>