<?xml version="1.0" encoding="utf-8"?><BoxLocalization xmlns="http://ferda.is-a-geek.net">
  <Identifier>DataMiningCommon.LiteralSetting</Identifier>
  <Label>Literal setting</Label>
  <Hint>A setting of basic attribute or its negation</Hint>
  <Categories>
    <Category>
      <Name>BooleanAttribute</Name>
      <Label>BooleanAttribute</Label>
    </Category>
  </Categories>
  <Sockets>
    <Socket>
      <Name>AtomSetting</Name>
      <Label>Atom </Label>
      <Hint>Defines atom setting.</Hint>
    </Socket>
    <Socket>
      <Name>LiteralType</Name>
      <Label>Literal type</Label>
      <Hint>Type of literal (Basic, Remaining)</Hint>
      <SelectOptions>
        <SelectOption>
          <Name>Basic</Name>
          <Label>Basic</Label>
          <ShortLabel>!</ShortLabel>
        </SelectOption>
        <SelectOption>
          <Name>Remaining</Name>
          <Label>Remaining</Label>
          <ShortLabel>?</ShortLabel>
        </SelectOption>
      </SelectOptions>
    </Socket>
    <Socket>
      <Name>GaceType</Name>
      <Label>Literal sign (gace)</Label>
      <Hint>Sign description (positive, negative, both).</Hint>
      <SelectOptions>
        <SelectOption>
          <Name>Positive</Name>
          <Label>Positive</Label>
          <ShortLabel>+</ShortLabel>
        </SelectOption>
        <SelectOption>
          <Name>Negative</Name>
          <Label>Negative</Label>
          <ShortLabel>-</ShortLabel>
        </SelectOption>
        <SelectOption>
          <Name>Both</Name>
          <Label>Both</Label>
          <ShortLabel>+/-</ShortLabel>
        </SelectOption>
      </SelectOptions>
    </Socket>
  </Sockets>
  <ModulesAskingForCreationSeq>
    <ModulesAskingForCreation>
      <Name>EquivalenceClass</Name>
      <Label>Class of equivalence</Label>
      <Hint>Adds attribute to a new Class of Equivalence box.</Hint>
    </ModulesAskingForCreation>
    <ModulesAskingForCreation>
      <Name>BooleanPartialCedentSetting</Name>
      <Label>Boolean partial cedent setting</Label>
      <Hint>Creates a new boolean partial cedent box.</Hint>
    </ModulesAskingForCreation>
  </ModulesAskingForCreationSeq>
</BoxLocalization>