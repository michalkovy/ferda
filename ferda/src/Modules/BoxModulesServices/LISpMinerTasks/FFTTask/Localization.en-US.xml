<?xml version="1.0" encoding="utf-8"?><BoxLocalization xmlns="http://ferda.is-a-geek.net">
  <Identifier>LISpMinerTasks.FFTTask</Identifier>
  <Label>4ft Task</Label>
  <Hint>Data mining procedure 4ft-Miner mines for various types of association rules. Rules based on statistical hypothesis test and conditional association rules are also included.</Hint>
  <Categories>
    <Category>
      <Name>FFTMiner</Name>
      <Label>4ft-Miner</Label>
    </Category>
  </Categories>
  <!--<Actions>
			<Action>
				<Name>Run</Name>
				<Label>Run</Label>
				<Hint>This will run 4ftMiner procedure (it means run generation of hypotheses)</Hint>
			</Action>
		</Actions>-->
  <Sockets>
    <Socket>
      <Name>AntecedentSetting</Name>
      <Label>Antecedent setting</Label>
      <Hint>Defines set of relevant antecedents.</Hint>
    </Socket>
    <Socket>
      <Name>SuccedentSetting</Name>
      <Label>Succedent setting</Label>
      <Hint>Defines set of relevant succedents.</Hint>
    </Socket>
    <Socket>
      <Name>ConditionSetting</Name>
      <Label>Condition setting</Label>
      <Hint>Defines set of relevant conditions.</Hint>
    </Socket>
    <Socket>
      <Name>FFTQuantifier</Name>
      <Label>4ft Quantifier</Label>
      <Hint>Defines 4ft quantifier for task</Hint>
    </Socket>
    <Socket>
      <Name>MissingsType</Name>
      <Label>Missings handling</Label>
      <Hint>Way of handling of missing values.</Hint>
      <SelectOptions>
        <SelectOption>
          <Name>Deleting</Name>
          <Label>Deleting</Label>
        </SelectOption>
        <SelectOption>
          <Name>PesimisticFillUp</Name>
          <Label>Pesimistic fill up</Label>
        </SelectOption>
        <SelectOption>
          <Name>OptimisticFillUp</Name>
          <Label>Optimistic fill up</Label>
        </SelectOption>
      </SelectOptions>
    </Socket>
    <Socket>
      <Name>Prolong100A</Name>
      <Label>Prolong 100% Antecedent</Label>
      <Hint>Include antecedent extensions of 100% implications</Hint>
    </Socket>
    <Socket>
      <Name>Prolong100S</Name>
      <Label>Prolong 100% Succedent</Label>
      <Hint>Include succedent extensions of 100% implications</Hint>
    </Socket>
    <Socket>
      <Name>IncludeSymetric</Name>
      <Label>Include symetry</Label>
      <Hint>Include symmetric hypotheses</Hint>
    </Socket>
    <Socket>
      <Name>GenerationState</Name>
      <Label>State</Label>
      <Hint>Determines if the generation of hypotheses is completed, interrupted or canceled.</Hint>
      <SelectOptions>
        <SelectOption>
          <Name>DidNotStart</Name>
          <Label>Didn't start</Label>
        </SelectOption>
        <SelectOption>
          <Name>Running</Name>
          <Label>Running</Label>
        </SelectOption>
        <SelectOption>
          <Name>Completed</Name>
          <Label>Completed</Label>
        </SelectOption>
        <SelectOption>
          <Name>Interrupted</Name>
          <Label>Interrupted</Label>
        </SelectOption>
        <SelectOption>
          <Name>Canceled</Name>
          <Label>Canceled</Label>
        </SelectOption>
      </SelectOptions>
    </Socket>
    <Socket>
      <Name>GenerationNrOfTests</Name>
      <Label>Number of verifications</Label>
      <Hint>Number of verifications during the generation.</Hint>
    </Socket>
    <Socket>
      <Name>GenerationNrOfHypotheses</Name>
      <Label>Hypotheses count</Label>
      <Hint>Number of generated hypotheses.</Hint>
    </Socket>
    <Socket>
      <Name>GenerationStartTime</Name>
      <Label>Start Time</Label>
      <Hint>Date and time when generation of task started.</Hint>
    </Socket>
    <Socket>
      <Name>GenerationTotalTime</Name>
      <Label>Total time</Label>
      <Hint>Total time of generation of hypotheses.</Hint>
    </Socket>
  </Sockets>
  <PropertyCategories>
    <PropertyCategory>
      <Name>Task Params</Name>
      <Label>Task Params</Label>
    </PropertyCategory>
    <PropertyCategory>
      <Name>Generating</Name>
      <Label>Generating</Label>
    </PropertyCategory>
  </PropertyCategories>
</BoxLocalization>